function toggleMenu() {
	let menu_toggle = document.getElementById("menu-toggle");
	if (menu_toggle.style.display === "flex") {
		menu_toggle.style.display = "none";
		menu_toggle.style.position = "fixed";
	} else {
		menu_toggle.style.display = "flex";
		menu_toggle.style.position = "fixed";
	}
}